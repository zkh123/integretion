CREATE TABLE `t_batchuserlist` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT '主键id',
  `user_id` int(11) NOT NULL COMMENT '用户id',
  `type` int(11) NOT NULL COMMENT '类型 ',
  `insert_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '插入时间',
  `update_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '更新时间',
  `is_active` tinyint(4) NOT NULL DEFAULT '1' COMMENT '逻辑删除 (1: 可用 ; 0: 删除)',
  PRIMARY KEY (`id`),
  KEY `idx_inserttime` (`insert_time`),
  KEY `idx_updatetime` (`update_time`)
) ENGINE=InnoDB AUTO_INCREMENT=1001 DEFAULT CHARSET=utf8mb4 COMMENT='用户问卷的记录/*owner=黄李俊 manager=吴一鸣*/';

CREATE TABLE `t_lk_user_list` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT '主键id',
  `user_id` int(11) NOT NULL COMMENT '用户id',
  `filter_type` int(11) NOT NULL COMMENT '类型',
  `user_type` int(11) NOT NULL COMMENT '用户类型 (1: app用户; 2: pc用户)',
  `insert_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '插入时间',
  `update_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '更新时间',
  `is_active` tinyint(4) NOT NULL DEFAULT '1' COMMENT '逻辑删除 (1: 可用 ; 0: 删除)',
  PRIMARY KEY (`id`),
  KEY `idx_inserttime` (`insert_time`),
  KEY `idx_updatetime` (`update_time`)
) ENGINE=InnoDB AUTO_INCREMENT=1001 DEFAULT CHARSET=utf8mb4 COMMENT='用户问卷的记录/*owner=黄李俊 manager=吴一鸣*/';

